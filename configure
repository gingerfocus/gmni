#!/bin/sh -e
srcdir=${SRCDIR:-$(dirname "$0")}
eval ". $srcdir/config.sh"

gmni() {
	genrules gmni \
		src/client.c \
		src/escape.c \
		src/gmni.c \
		src/tofu.c \
		src/url.c \
		src/util.c
}

gmnlm() {
	genrules gmnlm \
		src/client.c \
		src/escape.c \
		src/gmnlm.c \
		src/parser.c \
		src/tofu.c \
		src/url.c \
		src/util.c
}

all="gmni gmnlm"

run_configure

#!/bin/sh -e
srcdir=${SRCDIR:-$(dirname "$0")}
eval ". $srcdir/config.sh"

gmni() {
	genrules gmni \
		src/certs.c \
		src/client.c \
		src/escape.c \
		src/gmni.c \
		src/tofu.c \
		src/url.c \
		src/util.c
}

gmnlm() {
	genrules gmnlm \
		src/client.c \
		src/escape.c \
		src/gmnlm.c \
		src/parser.c \
		src/tofu.c \
		src/url.c \
		src/util.c
}

libgmni_a() {
	genrules libgmni.a \
		src/client.c \
		src/escape.c \
		src/tofu.c \
		src/url.c \
		src/util.c \
		src/parser.c
}

libgmni_pc() {
	:
}

all="gmni gmnlm libgmni.a libgmni.pc"


run_configure
